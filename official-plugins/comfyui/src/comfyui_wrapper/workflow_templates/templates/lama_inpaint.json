{
  "_meta": {
    "title": "LaMa Inpaint",
    "description": "Smart object removal using LaMa model",
    "version": "1.0.0",
    "requires": ["comfyui-inpaint-nodes"]
  },
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "LoadImage", 
    "inputs": {
      "image": "{{mask_path}}"
    }
  },
  "3": {
    "class_type": "INPAINT_LoadInpaintModel",
    "inputs": {
      "model_name": "{{model_name}}"
    }
  },
  "4": {
    "class_type": "INPAINT_InpaintWithModel",
    "inputs": {
      "inpaint_model": ["3", 0],
      "image": ["1", 0],
      "mask": ["6", 0],
      "seed": "{{seed}}"
    }
  },
  "5": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "dawnchat_lama",
      "images": ["4", 0]
    }
  },
  "6": {
    "class_type": "InvertMask",
    "inputs": {
      "mask": ["2", 1]
    }
  }
}

