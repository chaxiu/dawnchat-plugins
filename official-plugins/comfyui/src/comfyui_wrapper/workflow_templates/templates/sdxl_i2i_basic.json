{
  "_meta": {
    "title": "SDXL Image-to-Image Basic",
    "description": "Basic SDXL image-to-image workflow",
    "version": "1.0.0"
  },
  "3": {
    "class_type": "KSampler",
    "inputs": {
      "cfg": "{{cfg_scale}}",
      "denoise": "{{strength}}",
      "latent_image": ["11", 0],
      "model": ["4", 0],
      "negative": ["7", 0],
      "positive": ["6", 0],
      "sampler_name": "euler",
      "scheduler": "normal",
      "seed": "{{seed}}",
      "steps": "{{steps}}"
    }
  },
  "4": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "{{model_name}}"
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["4", 1],
      "text": "{{prompt}}"
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["4", 1],
      "text": "{{negative_prompt}}"
    }
  },
  "8": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["3", 0],
      "vae": ["4", 2]
    }
  },
  "9": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "dawnchat_i2i",
      "images": ["8", 0]
    }
  },
  "10": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "11": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["10", 0],
      "vae": ["4", 2]
    }
  }
}