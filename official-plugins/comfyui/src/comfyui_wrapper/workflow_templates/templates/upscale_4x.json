{
  "_meta": {
    "title": "RealESRGAN 4x Upscale",
    "description": "4x upscaling using RealESRGAN",
    "version": "1.0.0"
  },
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "UpscaleModelLoader",
    "inputs": {
      "model_name": "{{model_name}}"
    }
  },
  "3": {
    "class_type": "ImageUpscaleWithModel",
    "inputs": {
      "upscale_model": ["2", 0],
      "image": ["6", 0]
    }
  },
  "4": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "dawnchat_upscale",
      "images": ["3", 0]
    }
  },
  "5": {
    "class_type": "ImageInvert",
    "inputs": {
      "image": ["1", 0]
    }
  },
  "6": {
    "class_type": "ImageInvert",
    "inputs": {
      "image": ["5", 0]
    }
  }
}

